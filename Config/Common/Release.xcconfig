#include "Config/Common/Base.xcconfig"

// Architectures
ONLY_ACTIVE_ARCH = NO

// Build Options
DEBUG_INFORMATION_FORMAT = dwarf-with-dsym
ENABLE_TESTABILITY = NO

// Signing
CODE_SIGN_IDENTITY = $(CODE_SIGN_IDENTITY_RELEASE)
PROVISIONING_PROFILE_SPECIFIER = $(PROVISIONING_PROFILE_SPECIFIER_RELEASE)

// Apple Clang - Code Generation
GCC_OPTIMIZATION_LEVEL = $(inherited)

// Apple Clang - Preprocessing
ENABLE_NS_ASSERTIONS = NO
GCC_PREPROCESSOR_DEFINITIONS = SENTRY_ENABLED=$(SENTRY_ENABLED) $(inherited)

// User-Defined
MTL_ENABLE_DEBUG_INFO = NO
SENTRY_ENABLED = 1
